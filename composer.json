{
    "name": "jlm/serializer-expression-bundle",
    "type": "symfony-bundle",
    "description": "Allows you to use Symfony expressions in a new runtime exclusion strategy",
    "keywords": ["serialization", "serialize", "jmsserializer"],
    "homepage": "http://github.com/jmcclell/JLMSerializerExpressionBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Jason McClellan",
            "email": "jason@jasonmcclellan.io"
        }
    ],
    "require": {
        "php": ">=5.3.2",
        "jlm/serializer-expression": "dev-master",                
        "symfony/framework-bundle": "~2.4||~3.0",
        "symfony/security-bundle": "~2.4||~3.0"
    },
    "require-dev": {
        "friendsofsymfony/rest-bundle": "~1.3",
        "symfony/finder": "~2.4||~3.0",
        "symfony/browser-kit": "~2.4||~3.0",
        "jms/serializer-bundle": "~0.13||~1.0",
        "symfony/twig-bundle": "~2.4||~3.0",
        "sensio/framework-extra-bundle": "~3.0",
        "phpunit/phpunit": "~4.0||~5.0",
        "satooshi/php-coveralls": "dev-master"
    },
    "suggest": {
        "friendsofsymfony/rest-bundle": "~1.3"
    },
    "autoload": {
        "psr-0": { "JLM\\SerializerExpressionBundle": "" }
    },
    "minimum-stability": "dev",
    "target-dir": "JLM/SerializerExpressionBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
